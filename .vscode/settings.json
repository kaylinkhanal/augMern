{
  "debug.javascript.debugByLinkOptions": "on",
  "files.autoSave": "afterDelay",
  "editor.wordWrap": "on",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.experimentalInlineEdit.enabled": true,
  "editor.linkedEditing": true,
  "editor.mouseWheelZoom": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "diffEditor.codeLens": true,
  "editor.suggest.preview": true,
  "files.autoGuessEncoding": true,
  "workbench.editor.highlightModifiedTabs": true,
  "editor.lineHeight": 2,
  "accessibility.dimUnfocused.enabled": true,
  "accessibility.dimUnfocused.opacity": 0.9,
  "errorLens.fontStyleItalic": true,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.problemRangeDecorationEnabled": true,
  "errorLens.statusBarIconsEnabled": true,
  "errorLens.statusBarMessageEnabled": true,
  "errorLens.statusBarColorsEnabled": true,
  "editor.tabSize": 2,
  "notebook.formatOnSave.enabled": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "notebook.defaultFormatter": "esbenp.prettier-vscode",
  "editor.stickyScroll.scrollWithEditor": true,
  "editor.stickyScroll.enabled": false,
  "javascript-repl.coverage": false,
  "javascript-repl.outputChannel": true,
  "javascript-repl.useWorkers": true,
  "javascript-repl.usageEmptyContent": false
}
